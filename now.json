{
  "name": "valerio-portfolio",
  "version": 2,
  "alias": ["valeriosillari.com", "www.valeriosillari.com"],
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "build": {
    "env": {
      "GOOGLE_ANALYTICS_KEY": "@valerio_portfolio_google_analytics_key"
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/policy",
      "dest": "/policy.html"
    },
    {
      "src": "/.*",
      "status": 404,
      "dest": "error.html"
    }
  ]
}
